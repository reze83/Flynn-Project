{
  "name": "@flynn/cli",
  "version": "1.0.0",
  "description": "Flynn CLI - Command-line interface for Flynn Expert System",
  "type": "module",
  "bin": {
    "flynn": "./bin/flynn.js"
  },
  "files": ["dist", "bin", "README.md"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
