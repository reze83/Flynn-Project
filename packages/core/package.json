{
  "name": "@flynn/core",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "bundle": "tsup src/index.ts --format esm,cjs --dts --outDir dist"
  },
  "dependencies": {
    "@mastra/core": "^0.24.6",
    "@mastra/mcp": "^0.14.0",
    "minimatch": "^9.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "yaml": "^2.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
