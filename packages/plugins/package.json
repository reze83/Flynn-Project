{
  "name": "@flynn/plugins",
  "version": "1.0.0",
  "description": "Flynn Plugin System - Extensible plugin architecture for Flynn",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
